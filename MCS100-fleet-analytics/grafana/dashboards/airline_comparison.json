          "rawQuery": true,
          "rawSql": "SELECT\n  airline_code as \"Compagnie\",\n  COUNT(DISTINCT aircraft_msn) as \"Nombre d'avions\",\n  AVG(avg_daily_flight_hours) as \"Heures de vol quotidiennes\",\n  AVG(maintenance_events_per_flight_hour) as \"Événements de maintenance par heure de vol\",\n  AVG(alerts_per_flight_hour) as \"Alertes par heure de vol\",\n  AVG(unscheduled_maintenance_ratio) as \"Ratio de maintenance non programmée\"\nFROM\n  metrics.performance_metrics\nGROUP BY\n  airline_code\nORDER BY\n  \"Heures de vol quotidiennes\" DESC",
